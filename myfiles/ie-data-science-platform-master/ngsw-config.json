{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ],
        "urls": [
          "https://fonts.googleapis.com/**"    
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
        "name": "34.227.111.153:8080",
        "urls": [          
          "http://34.227.111.153:8080/**"   
        ],
        "cacheConfig": {
            "maxSize": 100,
            "maxAge": "7d",
            "timeout": "10s",
            "strategy": "freshness"
        }
    }
]
}
